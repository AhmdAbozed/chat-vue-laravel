{
    "version": 2,
      "framework": null,
    "builds": [
        { "src": "/api/index.php", "use": "vercel-php@0.7.0" },
        { "src": "/public/**", "use": "@vercel/static" }
    ],
    "routes": [
        
        {
            "src": "/build/assets/(.*)",
            "dest": "/public/build/assets/$1"
        },
    {
        "src": "/(.*)",
        "dest": "/api/index.php"
    }]
}